///////////////////////////////////////////////

int SFInt32Token() :
{
	Token	t;
	int		value; 
}
{
	t=<NUMBER>
		{
			value = Integer.parseInt(t.image);
			addSFInt32(value);
			return value;
		}
|	t=<HEX_NUMBER>
		{
			value = Integer.parseInt(new String(t.image.getBytes(), 2, (t.image.length() - 2)), 16);
			addSFInt32(value);
			return value;
		}
}

