void SFVec2fToken(float value[]) :
{
	float x, y;
}
{
	x=SFFloatToken() y=SFFloatToken()
		{
			value[0] = x;
			value[1] = y;
			addSFVec2f(value);
//			System.out.println("MFVec3fToken : " + value[0] + ", "+ value[1] + ", " + value[2] );
		}
}

void MFVec2fToken() :
{
	float value[] = new float[2];
}
{
//	"[" (SFVec2fToken(value) ( "," SFVec2fToken(value) )*)* "]"
	"[" (SFVec2fToken(value) (",")*)* "]"
|	SFVec2fToken(value)
}

